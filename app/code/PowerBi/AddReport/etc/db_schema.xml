<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="rutavity_reports" resource="default" engine="innodb" comment="rutavity_reports Table">
		<column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id" />
		<column name="report_id" nullable="true" xsi:type="varchar" comment="report_id" length="500" />
		<column name="report_name" nullable="true" xsi:type="varchar" comment="report_name" length="500" />
		<column name="report_link" nullable="true" xsi:type="varchar" comment="report_link" length="500" />
		<column name="report_description" nullable="true" xsi:type="varchar" comment="report_description" length="500" />
		<column xsi:type="timestamp" name="time_stamp" on_update="false" nullable="false" default="CURRENT_TIMESTAMP" comment="Time Stamp" />
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="entity_id" />
		</constraint>
	</table>
</schema>
